{
	"expo": {
		"name": "Health Wrapped",
		"slug": "health-wrapped",
		"scheme": "healthwrapped",
		"version": "1.3",
		"orientation": "portrait",
		"icon": "./assets/icon.jpg",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"backgroundColor": "#121212"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.adetunji.healthwrapped",
			"infoPlist": {
				"NSHealthShareUsageDescription": "Read health data to track your progress.",
				"NSHealthUpdateUsageDescription": "Update health data to track your progress.",
				"ITSAppUsesNonExemptEncryption": false
			},
			"entitlements": {
				"com.apple.developer.healthkit": true
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.jpg",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"package": "com.adetunji.healthwrapped"
		},
		"plugins": [
			[
				"@kingstinct/react-native-healthkit",
				{
					"NSHealthShareUsageDescription": "Read health data to track your progress.",
					"NSHealthUpdateUsageDescription": "Update health data to track your progress.",
					"background": true
				}
			],
			"expo-router",
			"expo-build-properties",
			"expo-quick-actions"
		],
		"extra": {
			"router": {},
			"eas": {
				"projectId": "a515f677-9aa6-4381-a6c0-e6e05d48643c"
			}
		}
	}
}
